{
  "name": "ghostzero/tmi",
  "description": "PHP Twitch Messaging Interface",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "René Preuß",
      "email": "rene@preuss.io"
    }
  ],
  "autoload": {
    "psr-4": {
      "GhostZero\\Tmi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "react/socket": "^1.6",
    "ext-mbstring": "*",
    "ext-json": "*",
    "ext-swoole": "*",
    "swoole/ide-helper": "^4.6",
    "ext-sockets": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
